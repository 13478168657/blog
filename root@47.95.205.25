INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5730', '3', '0', '2', '0', '0', '', '5730', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507687913', '1507687913', '1', '0', '0', '', '/archives/5730.html', '1', '1', '1', '', '健身男神女神高清图片引爆眼球', '1507687512', '/d/file/pic/2017-10-11/022d9c3206c39be4dd17547683c3218e.jpg', '', '肌肉健身干货——清晰的线条，将美学带入建身界,免费为您定制专属您的健身计划。健身教练网推出一系列高清极品健身大神，女神。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5721', '1', '0', '63', '0', '0', '', '5721', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507605539', '1508476445', '1', '0', '0', '', '/archives/5721.html', '1', '1', '1', '', '现在健身教练的发展趋势如何', '1507605119', '/d/file/jiaolian/2017-10-10/80e24b34be81882aa1d88bd07114e091.jpg', '', '现在健身教练的发展趋势如何？最近几年健身行业发展十分迅速，其中健身教练这个职业吸引了很多年轻人的眼球，新一代的年轻人在受到西方文化的影响下长大，特别崇拜西方健康时尚的生活方式，而健身教练这个职业正好完全符合这样的观念。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5848', '13', '0', '10', '0', '0', '', '5848', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508491285', '1508491364', '1', '0', '0', '', '/archives/5848.html', '1', '1', '1', '', '健身教练培训学校可以考国职吗', '1508491150', '/d/file/peixun/2017-10-20/8c1c4eecc5ba97d4d9d2170a3669f889.jpg', '', '自从健身行业在我国兴起以来，国家体育总局对拥有国职证书考核资质的学校进行了更加严格的筛选，使得能够考取国职证书考核资质的学校更是少之又少，而健身教练网就是其中一所拥有国职证书考核资质的学校。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5847', '13', '0', '1', '0', '0', '', '5847', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508491147', '1508491147', '1', '0', '0', '', '/archives/5847.html', '1', '1', '1', '', '去健身学校学健身教练怎么样', '1508490951', '/d/file/peixun/2017-10-20/e736c71a75d67a70f8d5a6694383f1a5.jpg', '', '大家想要做健身教练，最好到专业的健身教练培训学校去学习。因为目前我国没有什么直接对口的健身专业，或者专业的健身大学，现在想要从事到健身行业当中来，都是通过专业的健身教练培训学校，经过全面的培训，考取到权威的证书……', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5846', '13', '0', '0', '0', '0', '', '5846', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508490949', '1508490949', '1', '0', '0', '', '/archives/5846.html', '1', '1', '1', '', '学健身教练为什么要参加培训', '1508490707', '/d/file/peixun/2017-10-20/edd6457911033097ddc6d2ab319e3527.gif', '', '随着中国城市化的高速发展与对健康与高品质生活的追求不断升级，经过近十余年的发展，成就了目前健身产业这一庞大市场。而基于健身行业良好的就业前景，优厚的薪水待遇，越来越多的人想要走上健身道路。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5845', '13', '0', '0', '0', '0', '', '5845', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508490704', '1508490704', '1', '0', '0', '', '/archives/5845.html', '1', '1', '1', '', '健身教练初学者入门需要学习哪些培训课程', '1508490594', '/d/file/peixun/2017-10-20/cc43faca1e2f804d488bad7edca8f854.jpg', '', '现在健身行业越来越受到大家的推崇，从而也就发展的愈加快速，使得健身教练成为了当下社会中既时尚又高薪的职业，得到了众人的认可，许多人开始学习健身技能知识，由于我国之前并不重视健身意识的培养，所以许多都是健身方面的小白，那身为初学者应该从哪些课程入手?', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5844', '13', '0', '0', '0', '0', '', '5844', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508490592', '1508490592', '1', '0', '0', '', '/archives/5844.html', '1', '1', '1', '', '经过了健身教练培训以后，就能在行业赚到钱吗', '1508490423', '/d/file/peixun/2017-10-20/72bae8410940bd5df9fdba0fb5ad9cbd.jpg', '', '很多想学习健身教练的人，都会有各种的顾虑，列如：担心健身教练的发展、担心健身教练的工作问题，怕不好找工作、担心年龄的问题，担心健身教练是一个吃青春饭的、担心工资的问题，怕健身教练赚不到什么钱、担心卖课的问题，认为是销售得靠口才、担心证书的问题，非常担心证书考不过。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5843', '13', '0', '0', '0', '0', '', '5843', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508489050', '1508489629', '1', '0', '0', '', '/archives/5843.html', '1', '1', '1', '', '从哪几个方面选择健身教练培训学校', '1508488949', '/d/file/peixun/2017-10-20/e92b14de8461bcfa6f3455bec2a0831b.jpg', '', '随着健身行业不断的发展，行业对于专业的健身教练需求量也越来越大，好多的年轻人也是看好这个行业发展情景，想学习健身教练，那么从哪几个方面选择健身教练培训学校呢？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5842', '15', '0', '16', '0', '0', '', '5842', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508384008', '1508384008', '1', '0', '0', '', '/archives/5842.html', '1', '1', '1', '', '引体向上健身器材有哪些', '1508383861', '/d/file/qicai/2017-10-19/c043b28e34cfe9a45853bdb71777c628.jpg', '', '引体向上的练习对锻炼肌肉以及调理身体非常有帮助，但哪些健身器材是能够进行引体向上的呢？引体向上应该怎么练才有好效果呢？练习引体向上过程当中又该注意什么呢？跟着健身教练网一起学习下！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5833', '15', '0', '0', '0', '0', '', '5833', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508381554', '1508381554', '1', '0', '0', '', '/archives/5833.html', '1', '1', '1', '', '你需要知道的女士健身练器械举重的好处', '1508381449', '/d/file/qicai/2017-10-19/06ef5d75fda0aa9c202cbaa649792ef3.jpg', '', '也许你会发现，健身房大多数大型器械背男士使用者，大多数女性都倾向瑜伽和小力量器械，你也很清楚专业的健身女士，健身达人，明星都在使用大型器械进行健身运动。你知道这是为什么吗？只有知道对自身的好处才会更加努力。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5841', '15', '0', '3', '0', '0', '', '5841', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508383858', '1508383858', '1', '0', '0', '', '/archives/5841.html', '1', '1', '1', '', '家庭减肥健身的健身器材有哪些？', '1508383738', '/d/file/qicai/2017-10-19/88302e1fc7831af50b43642c12ba0006.jpg', '', '随着社会的不断发展，越来越多的人开始注重自己的身体健康，但是随着工作生活节奏不断加快，大部分人都很少有时间去专业的健身房进行锻炼自己的身体，只有空闲时间在家中进行小幅度的运动，所以家庭健身成为很多人的首选场所。下面由我们男神女生吧小编我是男神为大家推荐几种适合在家中锻炼的健身器材。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5840', '15', '0', '0', '0', '0', '', '5840', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508383359', '1508383379', '1', '0', '0', '', '/archives/5840.html', '1', '1', '1', '', '如何才能拥有XXL尺寸的胸肌？你的各项健身训练都到位了吗？', '1508383110', '/d/file/qicai/2017-10-19/2ebe18bb9457f7109422be8176a33006.gif', '', '看着别人拥有超大号的胸肌你羡慕了吗?看着别人的胸肌大到可以跟着音乐节奏一起抖动你嫉妒了吗？看着自己松垮下垂的胸你自卑了吗?就比如平常生活中，男人会喜欢大胸的女人，反之，当然也有女人会喜欢大胸的男人。强大的胸肌是可以很好地展现男性魅力的，还不抓紧锻炼你的胸肌你还在发什么呆？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5782', '1', '0', '9', '0', '0', '', '5782', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508225070', '1508225070', '1', '0', '0', '', '/archives/5782.html', '1', '1', '1', '', '健身教练应该怎么带会员，更健康更接地气', '1508224933', '/d/file/jiaolian/2017-10-17/65572c31d898af60649dd19d20640fbe.jpg', '', '对于现在在健身房工作的健身教练，基本上思想和想法都会被禁锢的。而且大部分的健身教练很难把自己所学习的理论知识和实践想结合起来。尤其是在带会员进行训练的时候，很容易会把自己经常训练的方法，用到会员的身上。这种做法就犹如我们家里的大人一样，总是希望把自己认为好的东西，强加在别人的身上。这样就会出现许多的问题，无法很好的帮助会员提高自身的身体素质。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5781', '1', '0', '5', '0', '0', '', '5781', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508224930', '1508224930', '1', '0', '0', '', '/archives/5781.html', '1', '1', '1', '', '怎样才能成为健身教练', '1508224889', '/d/file/jiaolian/2017-10-17/019784107b861d172c15042e88296060.jpg', '', '随着健身行业发展越来越快，行业对从事健身工作的人才也十分紧缺，很多人问我，到底什么样的人才能成为健身教练呢？下面由我为大家一一解答。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5756', '29', '0', '0', '0', '0', '', '5756', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507799318', '1507799318', '1', '0', '0', '', '/archives/5756.html', '1', '1', '1', '', '2017年的健身教练资格证怎么考', '1507799161', '/d/file/zigezheng/2017-10-12/af0bffa38d4efb914e327d2f034601f7.jpg', '', '健身教练资格证书在国内颁发的机构有：国家体育总局、健身协会、培训学校，还有些是外来机构颁发的！在健身教练行业没有必要将各个机构颁发的健身教练证书都拿到手，只需要后的健身行业认可的证书就可以了。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5824', '15', '0', '0', '0', '0', '', '5824', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508315638', '1508315638', '1', '0', '0', '', '/archives/5824.html', '1', '1', '1', '', '健身器材使用讲究科学合理的方法', '1508315480', '/d/file/qicai/2017-10-18/6c76a8d39395fe9e6cd00f7120205caf.jpg', '', '很多男生喜欢去健身馆去健身，有强壮有力的身体是每个男人的梦想，那么健身器材使用注意事项有哪些呢?男士必备家用健身器材又有哪些呢?下面让我们一起来了解一下吧。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5823', '15', '0', '0', '0', '0', '', '5823', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508315478', '1508315478', '1', '0', '0', '', '/archives/5823.html', '1', '1', '1', '', '家用健身器材——腹肌轮的使用方法！', '1508315337', '/d/file/qicai/2017-10-18/f1e086c23c5ee37b8a660258be78ee22.jpg', '', '腹肌轮，又名健腹轮，顾名思义就是主要用来锻炼腹肌的一种健身器具。以其高强度、见效快而在广大健身爱好者中深受欢迎。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5822', '15', '0', '5', '0', '0', '', '5822', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508315268', '1508315285', '1', '0', '0', '', '/archives/5822.html', '1', '1', '1', '', '多种健身器材的使用方法 器械坐姿下拉背+坐姿推胸器', '1508314952', '/d/file/qicai/2017-10-18/ce5866f66229356c997f83a5b52d4678.jpg', '', '如今，很多白领都喜欢到健身房去锻炼身体，进入健身房锻炼已经成为一种潮流。那么当你初次进入健身房时面对各种不同的健身器材会不会不知所措呢？为了避免第一次去健身房锻炼对健身器材不熟悉的尴尬局面，就来看看健身教练网为您介绍的不用健身器材使用方法吧。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5821', '15', '0', '5', '0', '0', '', '5821', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508314949', '1508314949', '1', '0', '0', '', '/archives/5821.html', '1', '1', '1', '', '健身器材的使用方法', '1508314775', '/d/file/qicai/2017-10-18/209a69d27a1c5a25ad6d0de08af05c83.jpg', '', '不知道您有没有碰到这种情况，进了健身房感觉一片陌生，没有私人教练，也就只能玩玩跑步机或者上上团课，但是即使是跑步机也同样感觉，不会用呢！健身教练网来解救你了，今天咱就来说一下，健身器材那些事。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5820', '15', '0', '0', '0', '0', '', '5820', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508314772', '1508314772', '1', '0', '0', '', '/archives/5820.html', '1', '1', '1', '', '原来你的动作一直都是错的？快看看哑铃真正的使用方法吧！', '1508314651', '/d/file/qicai/2017-10-18/5c29377b246b7b63903f18a3f885f5fc.gif', '', '哑铃对于很多人来说绝对是一个健身神器，它吸引人的地方不仅仅只是使用它后能达到的非常棒的健身效果，更是因为它是健身器材里功能强大的一款神器，它能锻炼到人体的各个主要肌群。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5750', '25', '0', '12', '0', '0', '', '5750', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507790703', '1507791159', '1', '0', '0', '', '/archives/5750.html', '1', '1', '1', '', '想练肌肉该怎么吃？看健身教练如何推荐7个私房健身食谱', '1507790439', '/d/file/shipu/2017-10-12/428ca86532ec13d066470085940f12c6.jpg', '', '「想练肌肉该怎么吃？」一直是许多人的困扰，看着网路上各式各样的资料，常常是越看越疑惑。其实这些营养补充方式都是奠基在基本小原则与简单的食物上。只要你吃得对，身体就会给你对的反馈！以下是健身教练在自家厨房必备的七个肌壮小食谱，不只营养满点，更是美味得很！动手在家试试看，终结高油、高盐、高糖的不健康外食习惯，搭配规律渐进的运动习惯，肌壮身材不是梦！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5749', '25', '0', '25', '0', '0', '', '5749', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507788957', '1507789034', '1', '0', '0', '', '/archives/5749.html', '1', '1', '1', '', '健身增肌食谱，增肌时候吃这些才能不长肥肉长肌肉', '1507788662', '/d/file/shipu/2017-10-12/d8ae5f73cda6ceed4f2184ff5b052b35.jpg', '', '相信很多健身时间比较久的小哥哥们都知道，想要快速的练出胸肌、腹肌是离不开食物的补充，下面健身教练网教你增肌时候吃怎么样吃才能不长肥肉长肌肉，有想要增肌的小哥哥们记得收藏哦！', '0');

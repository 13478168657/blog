INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5764', '2', '0', '13', '0', '0', '', '5764', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507882395', '1507882395', '1', '0', '0', '', '/archives/5764.html', '1', '1', '1', '', '一周增肌计划安排，入门者最合适的健身方法！', '1507881699', '/d/file/jihua/2017-10-13/9869c9412f627276d1c6dd19858e4e5e.jpg', '', '健身成为一种时尚早已经是大势所趋，爱上健身的人正在与日俱增，尤其是一些经常需要上镜的明星，为了能在观众面前展示更好的自己，一副好身材是必须的。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5763', '2', '0', '2', '0', '0', '', '5763', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507881489', '1507881573', '1', '0', '0', '', '/archives/5763.html', '1', '1', '1', '', '效果奇佳的一周健身锻炼计划', '1507881202', '/d/file/jihua/2017-10-13/330cec2f9569ff0f67b96fec821d12e2.jpg', '', '一周健身锻炼计划，让肌肉匀称生长每天重复的训练肯定会让你对健身的兴趣越来越低，所以给健身制定一个计划，每天都可以训练到不同的部位。以下动作根据个人能力，每组做15-20个即可，也可以每组做到力竭程度，组间休息一分钟。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5762', '28', '0', '3', '0', '0', '', '5762', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507801604', '1507801604', '1', '0', '0', '', '/archives/5762.html', '1', '1', '1', '', '徒手健身不需太多动作，5个动作提高基础能力', '1507801369', '/d/file/tushou/2017-10-12/041f65212ac69b6890168b50c98529aa.gif', '', '徒手健身不需要太多动作，这5个动作就够了！有个人认为，健身动作越多，越花俏，健身效果就会更好。其实并不是，健身只要做好几个动作就足够了。下面这些动作，让你的健身基础能力更加强！同时也增强身体灵活性和协调性！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5761', '28', '0', '2', '0', '0', '', '5761', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507801365', '1507801801', '1', '0', '0', '', '/archives/5761.html', '1', '1', '1', '', '徒手健身初期正确训练方法', '1507801155', '/d/file/tushou/2017-10-12/c17675e88409f2a91718ec2925432aa2.jpg', '', '作为一名徒手健身小白需要了解的知识。健身教练网整理了几套方案供大家参考。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5760', '28', '0', '3', '0', '0', '', '5760', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507800807', '1507800807', '1', '0', '0', '', '/archives/5760.html', '1', '1', '1', '徒手健身', '在家徒手健身就是这9个动作，让你一天帅24小时', '1507800312', '/d/file/tushou/2017-10-12/61d7ca601c73b5d857b6751f2a3b7c2a.jpg', '', '怎样变的更加帅气，到底怎么办，健身教练网教你徒手健身技巧，让你瞬间变帅哥在家徒手健身就是这9个动作，让你一天帅24小时！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5759', '28', '0', '14', '0', '0', '', '5759', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507800310', '1507800893', '1', '0', '0', '', '/archives/5759.html', '1', '1', '1', '', '增肌徒手健身 每天在家10分钟练遍全身', '1507800050', '/d/file/tushou/2017-10-12/5bd69b83d1ef2f8c6e5e72be68b78814.gif', '', '爱生活，爱健身。健身难吗？不难，只要有心随时随地都可以健身。今天，健身教练网为大家推荐一套不受场地、不受器械的室内徒手健身动作，每天10分钟练遍全身。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5758', '29', '0', '1', '0', '0', '', '5758', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507799657', '1507799657', '1', '0', '0', '', '/archives/5758.html', '1', '1', '1', '', '2017健身教练职业资格证怎么考', '1507799435', '/d/file/zigezheng/2017-10-12/3d6513efb3f2fd19979cd1214d5dcf41.jpg', '', '健身教练职业资格证书是健身教练进去健身行业的敲门砖，健身教练职业资格证是怎么考取的呢？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5757', '29', '0', '0', '0', '0', '', '5757', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507799432', '1507799432', '1', '0', '0', '', '/archives/5757.html', '1', '1', '1', '', '考健身教练资格证需要多少钱？', '1507799321', '/d/file/zigezheng/2017-10-12/a1883ffb87687a80a63f29b7e4f8d11d.jpg', '', '近些年健身行业的发展逐渐趋向正规化，考取健身教练资格证的人愈来愈多。考健身教练资格证多少钱？这似乎是大家想要知道的问题。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5803', '28', '0', '0', '0', '0', '', '5803', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508231058', '1508231747', '1', '0', '0', '', '/archives/5803.html', '1', '1', '1', '', '在公园如何徒手健身 公园徒手健身计划', '1508230482', '/d/file/tushou/2017-10-17/debd5af2dc1c8e2f74019a72fd149df0.jpg', '', '研究显示，在高低不平、变化多端的户外地形运动，经常有风来提高阻力，身体消耗的能量会显着超越穿着运动鞋在跑步机履带上啪答啪答地跑步，当然，速效运动是室内室外两相宜，但为了增添变化，将公园健身运动加进运动项目当中也是个不错的选择!下文的长椅运动有些是运动科学家史提夫.梅勒设计的，其馀的则是作者这些年来觉得运动效果良好的动作!', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5802', '28', '0', '0', '0', '0', '', '5802', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508230480', '1508230480', '1', '0', '0', '', '/archives/5802.html', '1', '1', '1', '', '徒手健身新高度复合型锻炼不能少', '1508230217', '/d/file/tushou/2017-10-17/76a2271b5e0877b09872037d3a25a2ab.gif', '', '习惯了动用全身的复合型锻炼，掌握了各类变式，你的体格能在其中获得更全面的发展，身体上每一小块肌肉都被锻炼得无懈可击，神经系统与肌肉之间的联结更加稳固，你将变得更强大！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5801', '28', '0', '0', '0', '0', '', '5801', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508230214', '1508230214', '1', '0', '0', '', '/archives/5801.html', '1', '1', '1', '', '徒手健身中，如何锻炼腿部', '1508230134', '/d/file/tushou/2017-10-17/8bac59d35ab2fc24b47514bbc0a95fb9.jpg', '', '我们知道，腿部的力量容易被健身者忽视，所以有高手练腿一说。结实，有力的双腿，对健康有非常大的好处。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5800', '28', '0', '0', '0', '0', '', '5800', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508230006', '1508230029', '1', '0', '0', '', '/archives/5800.html', '1', '1', '1', '', '徒手健身还是去健身房？看完你就知道了！', '1508229787', '/d/file/tushou/2017-10-17/a3e24a704f0c28c02d98065d42520305.jpg', '', '现在大家都有健身的需求你可能会犹豫到底是去健身房还是应该徒手健身别急老司机为你盘点两者利弊', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5799', '28', '0', '0', '0', '0', '', '5799', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508229785', '1508229785', '1', '0', '0', '', '/archives/5799.html', '1', '1', '1', '', '一组徒手燃脂动作，你坚持得下去吗？', '1508229431', '/d/file/tushou/2017-10-17/7871f857f6ed61583c9559782e8cbdd1.jpg', '', '在健身的道路上，有10%的人因为懒而不想开始，20%的人决心不够试了几天就放弃，30%的人坚持了两周以上，但因为各种原因而破功……分享一组徒手健身动作，全身燃脂的自重训练，瘦腰提臀，简单实用。每个动作16次，根据自身情况循环1-2次。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5798', '28', '0', '0', '0', '0', '', '5798', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508229428', '1508229428', '1', '0', '0', '', '/archives/5798.html', '1', '1', '1', '', '徒手健身减脂十五分钟见效', '1508229010', '/d/file/tushou/2017-10-17/29759b886d014e8fb784baef6230dd7d.jpg', '', '对于每天忙碌的上班族而言，每天15分钟的高效燃脂运动，不会让人感到疲惫，反而恰到好处。15分钟里选择针对性锻炼肌肉的运动，高效瘦身，让身形更具线条美。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5797', '28', '0', '0', '0', '0', '', '5797', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508229006', '1508229006', '1', '0', '0', '', '/archives/5797.html', '1', '1', '1', '', '徒手训练超强腹肌', '1508228828', '/d/file/tushou/2017-10-17/3ab2d2edb1f1b9248331c3b59d65c53a.gif', '', '8个动作，主要练腹肌，1-5个循环，练习前先进行5-10分钟的准备活动，练习过程中宁可慢一点也要将每一个动作做标准，动作与动作之间尽量不休息，全套动作结束后休息2分钟。具体如下：', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5796', '2', '0', '1', '0', '0', '', '5796', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508228348', '1508228348', '1', '0', '0', '', '/archives/5796.html', '1', '1', '1', '', '三个月减脂计划减肥瘦身20斤', '1508228200', '/d/file/jihua/2017-10-17/5ac21596b3bf8e7f014326dc9d8b1357.jpg', '', '今天健身教练网为即将成为减肥界新手的你准备了以下计划（画外音：加油！加油！）1 、提高体能阶段——1个月这一个月期间，（一周至少有4天的训练时间）力量训练20分钟左右，主要用来学习和掌握各个力量训练动作的要领和所锻炼的部位。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5795', '2', '0', '0', '0', '0', '', '5795', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508228004', '1508228004', '1', '0', '0', '', '/archives/5795.html', '1', '1', '1', '', '新手如何制定有效的增肌计划？怎么训练、怎么吃？', '1508227797', '/d/file/jihua/2017-10-17/b43df5fb3f2bf759f90d3d6435b54be4.jpg', '', '对于增肌，饮食也是不可缺少的重要部分。肌肉生长是要消耗能量的，没有足够的热量，就不可能保证肌肉的正常生长。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5794', '2', '0', '20', '0', '0', '', '5794', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508227794', '1508227794', '1', '0', '0', '', '/archives/5794.html', '1', '1', '1', '', '新手健身计划指南，授人以鱼不如授人以渔！', '1508227659', '/d/file/jihua/2017-10-17/df7dd19e8b04592d6373167f5e13b67d.jpg', '', '授人以鱼不如授人以渔，因此在这里 简单讲讲如何制定健身计划。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5793', '2', '0', '1', '0', '0', '', '5793', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508227510', '1508228035', '1', '0', '0', '', '/archives/5793.html', '1', '1', '1', '', '初学健身怎么练？一个适当的健身新手的计划', '1508227365', '/d/file/jihua/2017-10-17/187d74e1945b1a5c535e3cf270a36c74.gif', '', '一个适当的健身计划平均每周只能帮你减掉2磅。要想明显看到肌肉、体型的改变，必须经过至少6周的持续锻炼。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5792', '2', '0', '0', '0', '0', '', '5792', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508227362', '1508227362', '1', '0', '0', '', '/archives/5792.html', '1', '1', '1', '', '健身新手，短短7天初学计划', '1508227162', '/d/file/jihua/2017-10-17/70e58bb1f4c006ec47a1b5ee5e3dcfdb.jpg', '', '星期一，目标肌肉：胸，动作：平板哑铃飞鸟6组x10个、平板哑铃卧推5组x12个、俯卧撑：6组x力竭星期二，目标肌肉：背，动作：单臂哑铃划船7组x12个，健身新手，短短7天初学计划！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5791', '2', '0', '0', '0', '0', '', '5791', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508226986', '1508226986', '1', '0', '0', '', '/archives/5791.html', '1', '1', '1', '', '健身新手要如何制定一份自己的健身计划？', '1508226754', '/d/file/jihua/2017-10-17/d18fdca9548127ce202bde1b222fb51b.jpg', '', '随着健身运动的普及，越来越多的人们正在加入健身的行列。健身，以其强身健体塑造形体的作用正在风靡。那作为健身新手，我们又该如何制定自己的健身计划呢？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5790', '2', '0', '0', '0', '0', '', '5790', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508226752', '1508226752', '1', '0', '0', '', '/archives/5790.html', '1', '1', '1', '', '新手健身计划 解决新手怎么健身的问题', '1508226634', '/d/file/jihua/2017-10-17/6166bc2a79996c8d2ed3305ee3a877eb.gif', '', '大家可能在网上也看过不少健身计划，什么样都有。练5休一的(练5天休一天)，金字塔训练法，超级组什么的。我要说的是，每个人都有自己的训练法，适合自己最好，有些训练法只适合高水平运动员，普通人达不到人家那耐力，力量，效果不会好。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5789', '2', '0', '0', '0', '0', '', '5789', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508226632', '1508226632', '1', '0', '0', '', '/archives/5789.html', '1', '1', '1', '', '新手健身之前必须要了解到的一些基础知识', '1508226502', '/d/file/jihua/2017-10-17/935984749eb0d5f24d4c885508c3a5d8.jpg', '', '一个健身新手在刚踏入健身房的时候，看到健身房中的那些锻炼器械，往往总会感觉到十分的头大，当正式开始锻炼的时候，看着身边的人一个个在那练的汗如雨下，可是你却偏偏不知道该如何去锻炼，其实一个新手在刚去健身房的时候不知道如何去锻炼是正常的', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5788', '2', '0', '1', '0', '0', '', '5788', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508226500', '1508227088', '1', '0', '0', '', '/archives/5788.html', '1', '1', '1', '', '健身新手须知，教你如何制定健身计划，走好健身路上的第一步', '1508226269', '/d/file/jihua/2017-10-17/9b97ded898f60f942fce5f87bc1f6224.jpg', '', '现当今的人们去健身房，通常只是两个目的一个是减脂，一个是增肌。而在运动之前，各位定是会查阅相关的资料，什么类型的动作会怎样的刺激肌群之类的话题。大部分都在说明动作和方法，却没有说如何去制定和完成一个完美健身计划。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5787', '2', '0', '2', '0', '0', '', '5787', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508226266', '1508227101', '1', '0', '0', '', '/archives/5787.html', '1', '1', '1', '', '适合新手的高强度肌锻炼计划', '1508225945', '/d/file/jihua/2017-10-17/a438e091b638ea7a86a8fcb1b729f553.gif', '', '低强度运动减脂，高强度运动增肌目标为减肥的人，可以适当采用低强度的运动方式，如游泳、跑步等，这种方式对脂肪的消耗率大。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5786', '2', '0', '0', '0', '0', '', '5786', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508225942', '1508225942', '1', '0', '0', '', '/archives/5786.html', '1', '1', '1', '', '健身新手，如何合理安排健身计划？', '1508225682', '/d/file/jihua/2017-10-17/27cf526a161ca774b5f5eaabfe75764a.jpg', '', '健身是强身健体、提高自身心理素质的最好手段，有越来越多的人愿意加入到健身的行列中来。正确的健身训练由力量训练+有氧运动组成。先做力量训练，再做有氧运动，才能保证效果。健身前记得先做热身运动，健身后必须做拉伸！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5785', '2', '0', '1', '0', '0', '', '5785', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508225678', '1508227112', '1', '0', '0', '', '/archives/5785.html', '1', '1', '1', '', '新手计划|第一天健身训练初入门（自身体重训练）', '1508225465', '/d/file/jihua/2017-10-17/a3c54bafc6b90be3c2a245dae73ebcbb.jpg', '', '今天你的选择，就是让你的身体开始重生，通过健身变成更优秀的你！不管男女，无论你是减脂、塑形或者增肌。从这里开始！', '0');

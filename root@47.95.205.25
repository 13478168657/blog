INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5735', '3', '0', '33', '0', '0', '', '5735', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507692961', '1507692994', '1', '0', '0', '', '/archives/5735.html', '1', '1', '1', '', '为什么越来越多的中国女人热衷于健身？看完这些图片就懂了', '1507692652', '/d/file/pic/2017-10-11/a129c7977354f538195ac862db274bf2.jpg', '', '随着经济水平的提高，广大劳动人民的物质条件也开始好起来了。而伴随着物质条件逐渐好起来的一些证明就是，越来越多的中国人开始胖起来了！当然胖起来的不止是男人还有女人，有人说男人健身是为了练出一身好肌肉，从而让自己看着很男人，那么女人健身呢？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5734', '3', '0', '0', '0', '0', '', '5734', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507692446', '1507692446', '1', '0', '0', '', '/archives/5734.html', '1', '1', '1', '', '2017年流行于健身圈的8张图片，总有那么一张感动你', '1507692069', '/d/file/pic/2017-10-11/cbb9f60a8217aaa9a8ed4a1212f0117d.jpg', '', '　　作为一名著名的健美运动员，阿诺在自己19岁到69岁这50年的岁月里，创造了太多个一般人难以企及的高度！是一名职业的健美运动员，并曾几乎连续6年获得奥赛健美冠军；担任过电影中', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5733', '3', '0', '13', '0', '0', '', '5733', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507692066', '1507697503', '1', '0', '0', '', '/archives/5733.html', '1', '1', '1', '', '健身和不健身的区别，一组图片让你醒悟', '1507691625', '/d/file/pic/2017-10-11/65c0661a60d81c41e3e4acf63444bd0f.jpg', '', '他用了一年瘦小子变壮汉几年的时间，打造出一个肌肉男三年变成另一个人满肚肥肠是可以去掉的\r\n大腹便便也可以练出小馒头我们只需要努力，其它的就交给时间吧～～', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5732', '3', '0', '7', '0', '0', '', '5732', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507691623', '1507692498', '1', '0', '0', '', '/archives/5732.html', '1', '1', '1', '', '健身美女秀健美照片', '1507691295', '/d/file/pic/2017-10-11/63ee3780ec460ba1d6573b9ed7945599.jpg', '', '在健身房遇到美女秀照片是不是很冲动，下面我们看看这个身材棒棒的美女秀自己的健身过程中的照片吧。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5729', '2', '0', '9', '0', '0', '', '5729', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507626128', '1507881198', '1', '0', '0', '', '/archives/5729.html', '1', '1', '1', '', '正确的健身计划制定方法', '1507625863', '/d/file/jihua/2017-10-10/fd6aa3349a0d27d404662858defee0e5.jpg', '', '训练中遵循正确的训练流程可以预防运动损伤，让训练效率提高，更快提升运动表现与能力，以及更快实现你的训练目标。合理的训练流程需要由几部分构成：热身，功能性训练与放松。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5728', '1', '0', '18', '0', '0', '', '5728', '2', 'adminn.cn', '0', '1', '1', '0', '0', '0', '0', '1507625780', '1507629589', '1', '0', '0', '', '/archives/5728.html', '1', '1', '1', '', '健身房女会员那么多，为何还有健身教练单身？', '1507625492', '/d/file/jiaolian/2017-10-10/03271f1a48d2d88a2e6a118df5502274.jpg', '', '健身教练为何单身的很多？这边所说的是男教练。那事实上健身教练单身的多吗？真的很多，可以这样说，即使长的不错的健身教练的确是容易找到女朋友，但想找到真心陪他过日子的不容易。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5727', '1', '0', '4', '0', '0', '', '5727', '2', 'adminn.cn', '0', '1', '1', '0', '0', '0', '0', '1507625488', '1507629600', '1', '0', '0', '', '/archives/5727.html', '1', '1', '1', '', '健身为什么我们需要私教', '1507625259', '/d/file/jiaolian/2017-10-10/93eaa3888cdf3778bf919166ffe944af.jpg', '', '对于每个刚刚接触“健身”的新手，私人教练需要根据其身体各项指标，设置运动处方：运动形式、运动频率、运动量和强度、饮食建议等。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5726', '1', '0', '7', '0', '0', '', '5726', '2', 'adminn.cn', '0', '1', '1', '0', '0', '0', '0', '1507625257', '1507629659', '1', '0', '0', '', '/archives/5726.html', '1', '1', '1', '', '健身教练证书怎么考取', '1507625193', '/d/file/jiaolian/2017-10-10/55007b703b726be9ee685a9b1cc9a744.jpg', '', '随着我国健身行业的不断发展，越来越多的人投入到健身行业当中，这也需要对行业的规范有统一的严格的整治标准，做为健身房的顶梁柱健身教练来说，现在大多数健身教练都没有统一的标准，这也是的中国的健身行业目前的现状，正是因为这样，国家对健身这块越加的重视。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5725', '3', '0', '6', '0', '0', '', '5725', '2', 'adminn.cn', '1', '0', '1', '0', '0', '0', '0', '1507622323', '1507629707', '1', '0', '0', '', '/archives/5725.html', '1', '1', '1', '', '曼联名宿晒与伊布在健身房照片', '1507622249', '/d/file/pic/2017-10-10/b1935749b10c468d5dd03190c2622d76.jpg', '', '10月6日讯 近日，前曼联球星安迪-科尔在自己的Instagram账户上贴出了一张和伊布在健身房的照片，并配文“早上好，我的健身新伙伴！”照片中两人都身着曼联训练服。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5831', '15', '0', '0', '0', '0', '', '5831', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508381002', '1508381002', '1', '0', '0', '', '/archives/5831.html', '1', '1', '1', '', '你该选择使用什么样的健身器械', '1508380729', '/d/file/qicai/2017-10-19/56acb4a347f8e984d8d3b8dd732f6852.jpg', '', '在健身房中，各种健身错综林立，什么是适合你的？大多数人的脑子里是没有概念的，看别人使什么就使什么？看教练使什么就使什么？当个 “ 糊涂虫 ” 可不能拥有性感的身材，今天我们就健身器械的选择给大家做个具体分析。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5830', '15', '0', '0', '0', '0', '', '5830', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508380726', '1508380726', '1', '0', '0', '', '/archives/5830.html', '1', '1', '1', '', '奇迹健身，一把椅子就解决器械问题', '1508380584', '/d/file/qicai/2017-10-19/af3ab9c8485c495efbb2e746cea905ff.jpg', '', '人们想到健身最先的反应就是一大堆的器械，什么哑铃、杠铃、跑步机等等一大堆，但是今天小编要给你说的是奇迹健身，如果还是正儿八经的健身方法，还怎么称的上奇迹健身，它所有健身动作的器械知识一把凳子，奇怪吧，往下看。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5780', '1', '0', '10', '0', '0', '', '5780', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508224739', '1508224853', '1', '0', '0', '', '/archives/5780.html', '1', '1', '1', '', '健身教练必经之路', '1508224583', '/d/file/jiaolian/2017-10-17/724180c3da75faeab36dfedccac62642.jpg', '', '健身行业近年来的发展形势势不可挡，健身教练成为社会炙手可热的职业，已是板上钉钉的事情。做健身教练最需要的是什么呢？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5779', '1', '0', '4', '0', '0', '', '5779', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508224580', '1508224580', '1', '0', '0', '', '/archives/5779.html', '1', '1', '1', '', '做健身教练怎么样', '1508224452', '/d/file/jiaolian/2017-10-17/40d1262c41f4b3a894ed46c3eb2f1213.jpg', '', '做健身教练当然是很好的，这份职业即健康，有高薪，因为现在在的工作都是消耗身体在挣钱，而健身教练确实相反的，这份职业不仅能自己锻炼身体，享受快乐的健身，还能同时赚到高薪的工资，还能帮助更多的人得到健康的锻炼，传递健康、传递快乐。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5829', '15', '0', '0', '0', '0', '', '5829', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508380580', '1508380580', '1', '0', '0', '', '/archives/5829.html', '1', '1', '1', '', '健身房最全器械使用教学，帮你省一些私教费', '1508380224', '/d/file/qicai/2017-10-19/fcaec619ab83c0d1cdbf834ff2046461.gif', '', '健身房有很多的固定器械，但是若你不缴纳一些私教费用，教练也不会主动过来教你。不过当你能看完此文，就可以脱离健身新手期，省下最起码2000块的私交费用。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5778', '1', '0', '0', '0', '0', '', '5778', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508224450', '1508224864', '1', '0', '0', '', '/archives/5778.html', '1', '1', '1', '', '体坛最美健身女教练 身体和营养不好的就别进来了', '1508224340', '/d/file/jiaolian/2017-10-17/8c6cad2fab62abaed22f911f481362c9.jpg', '', '体坛最美健身女教练 身体和营养不好的就别进来了其实，咱们中国也有很多健身女教练很美，身材要远远好过芮呈和，只是还没有被发掘出来。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5828', '15', '0', '0', '0', '0', '', '5828', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508316125', '1508316125', '1', '0', '0', '', '/archives/5828.html', '1', '1', '1', '', '腰背按摩器——广场健身器材介绍', '1508316020', '/d/file/qicai/2017-10-18/7ffee8d79184703519cfb1bc9d4f93de.jpg', '', '腰背按摩器是常见的室外健身器材的一种，有放松背、腰部肌肉，消除肌肉疲劳，调节神经系统等功能。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5827', '15', '0', '5', '0', '0', '', '5827', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508316017', '1508316017', '1', '0', '0', '', '/archives/5827.html', '1', '1', '1', '', '坐蹬训练器——广场健身器材介绍', '1508315949', '/d/file/qicai/2017-10-18/f4d92a87ad916368686e48d5c2d02a7e.jpg', '', '坐蹬训练器也叫坐蹬器，多见于小区健身广场、体育广场、校园、公园等室外体育健身场所，常见的有双位和三位两种。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5826', '15', '0', '0', '0', '0', '', '5826', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508315946', '1508315946', '1', '0', '0', '', '/archives/5826.html', '1', '1', '1', '', '太空漫步机——广场健身器材介绍', '1508315805', '/d/file/qicai/2017-10-18/cb643310babf6a968b1cfe48c0a2b381.jpg', '', '太空漫步机是很常见的一种运动健身器材，通过类似于自然跨步的动作，使两腿交替自然摆动，从而提高人体有氧运动能力和身体的协调性、柔韧性，锻炼腿部肌肉力量，增进心肺功能，促进呼吸系统健康，增强平衡能力。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5777', '1', '0', '0', '0', '0', '', '5777', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508223320', '1508224336', '1', '0', '0', '', '/archives/5777.html', '1', '1', '1', '', '好的健身教练，百里挑一', '1508222755', '/d/file/jiaolian/2017-10-17/7a801b6dcd539e0e4045104891365aba.jpg', '', '找到合适的健身教练，和找到靠谱的对象一样，难。很多时候，我们的选择会被一些表面上的东西所迷惑，比如心目中的教练一定是高大威猛超大块肌肉，女生则是前凸后翘身材一级棒那种。关于什么是好的教练，不同的人心目中有不同标准。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5776', '1', '0', '2', '0', '0', '', '5776', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508222752', '1508222752', '1', '0', '0', '', '/archives/5776.html', '1', '1', '1', '', '只有健身教练，才能看懂的专业知识', '1508221750', '/d/file/jiaolian/2017-10-17/22fd70fa58ab55377f415580f6c1c3f3.jpg', '', '作为全能私人健身教练需要掌握的知识也是很多，不光是一些基础的知识，像cpr心肺复苏急救，运动康复等也是需要掌握的，想学习健身教练的请到健身教练网。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5775', '1', '0', '0', '0', '0', '', '5775', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508221748', '1508221748', '1', '0', '0', '', '/archives/5775.html', '1', '1', '1', '', '明星为什么健身效果这么快？还不是因为教练传授的这10条健身铁律', '1508221344', '/d/file/jiaolian/2017-10-17/6836af74774b38b90f18cb8edb79240d.jpg', '', '大家有没有发现，明星健身，那是真的快。由于有教练指导，他们会少走很多弯路，其实一般人健身，只要指导这10条健身铁律，也能很快见到成效的！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5825', '15', '0', '0', '0', '0', '', '5825', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508315802', '1508315802', '1', '0', '0', '', '/archives/5825.html', '1', '1', '1', '', '使用健身器材练腹肌的4个动作让腹肌半个月出来', '1508315642', '/d/file/qicai/2017-10-18/a92f3c8c10451631133c1996238c74cf.gif', '', '有读者问我，有没有很狠的锻炼腹肌方法，这对这个问题我个人认为应该是根据你目前锻炼状况来的，体脂已经降到开始显现腹肌是，腹肌撕裂者等都是高效率锻炼腹肌的方法。如果你常去健身房健身那你可以加入下面4个需要使用健身器材的腹肌锻炼方法：', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5819', '28', '0', '1', '0', '0', '', '5819', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508234557', '1508234557', '1', '0', '0', '', '/archives/5819.html', '1', '1', '1', '', '3个简单高效徒手锻炼你背肌的动作', '1508234348', '/d/file/tushou/2017-10-17/a8f2c729198d1faa7674c86015f673c4.jpg', '', '有句“新手练胸，老手练背！”时每个健身达人的口头禅，背部肌肉是是人体的一大肌群。对于增肌者而言必练的肌肉群。背肌分为背浅肌、背深肌和背部筋膜，锻炼背部应该使用不同的器械从上到下从不同的角度去锻炼它，达到即宽又厚。很多徒手健身大神使用的都是单杠，双杠来锻背肌的，对于我们普通在家锻炼的健身。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5818', '28', '0', '0', '0', '0', '', '5818', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508234345', '1508234345', '1', '0', '0', '', '/archives/5818.html', '1', '1', '1', '', '7个大神级的徒手动作，爆燃身体脂肪，打造完美的囚徒身形', '1508234270', '/d/file/tushou/2017-10-17/496678df60eb2072b9af1326e18c3167.jpg', '', '7个大神级的徒手动作，爆燃身体脂肪，打造完美的囚徒身形为了追求完美的体型，获得别人的赞美以及羡慕的眼光，人们总是在寻求多样的方法使得自己游泳完美的身材。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5817', '25', '0', '2', '0', '0', '', '5817', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508233876', '1508233919', '1', '0', '0', '', '/archives/5817.html', '1', '1', '1', '', '增肌减脂的食谱', '1508233739', '/d/file/shipu/2017-10-17/a967f0f8e70161023400a0e0535af058.jpg', '', '以上这些食谱只是一个建议，可以做一些替换，换成你自己喜欢的富含蛋白质脂肪较低的食物。\r\n', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5816', '25', '0', '1', '0', '0', '', '5816', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508233651', '1508233651', '1', '0', '0', '', '/archives/5816.html', '1', '1', '1', '', '秋冬健身 如何饮食有讲究', '1508233584', '/d/file/shipu/2017-10-17/8bb2bd1c9a7414d83f1b2a924c1a5b2f.jpg', '', '秋天对于许多减肥健身人士来说，绝对是个万恶的季节，在这个想吃想睡就睡不想动的季节，健身饮食计划绝对不能松懈，一定要跟肥肉斗争到底。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5815', '25', '0', '45', '0', '0', '', '5815', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508233493', '1508233493', '1', '0', '0', '', '/archives/5815.html', '1', '1', '1', '', '快看看这份来自地中海的健身食谱', '1508233256', '/d/file/shipu/2017-10-17/f7212a69301302fd6f2412964ce2b065.jpg', '', '健身教练网从专业的角度研究了很多食谱，并且亲测，这个来自地中海的健身食谱简单可行，效果显著，集天下健身食谱之大成。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5814', '25', '0', '19', '0', '0', '', '5814', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508233254', '1508233254', '1', '0', '0', '', '/archives/5814.html', '1', '1', '1', '', '吃鸡蛋，对健身有益，增肌效果明显的吃法，你知道有几种吗？', '1508233119', '/d/file/shipu/2017-10-17/d9b9cd0785ecb829956ebb6218f21830.jpg', '', '想要健身的人都是离不开的鸡蛋的，不管是想要增肌还是想要减脂，鸡蛋一直都是食谱上的必有食品，但是我们真的知道鸡蛋怎样吃才是最好的吗？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5813', '25', '0', '1', '0', '0', '', '5813', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508233116', '1508233116', '1', '0', '0', '', '/archives/5813.html', '1', '1', '1', '', '一日六餐增肥增肌食谱', '1508232988', '/d/file/shipu/2017-10-17/cda3e85e30b9fe437560fc03b3c0e589.jpg', '', '一日六餐增肥增肌食谱第一餐 7点-8点左右早餐碳水化合物：一个馒头、面包、花圈或米飯、面条均可(量稍大点)蛋白质：蛋白粉一杯、2个蛋清', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5812', '25', '0', '0', '0', '0', '', '5812', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232985', '1508232985', '1', '0', '0', '', '/archives/5812.html', '1', '1', '1', '', '瘦人健身增肌最佳食谱', '1508232919', '/d/file/shipu/2017-10-17/8bae1048cd62922c45430aab41b9a243.jpg', '', '要少吃燥热还有辛辣食品，如辣椒、蒜、姜、葱还有虾、蟹等助火散气的食物;酸冷食物也要少吃，如山楂、酸梅、橘子、柠檬、醋、生菜等;富含纤维还有不易消化的食物，如韭菜、芹菜还有高脂肪食物，由于容易让人产生饱腹感而减少食物的摄入量，所以也要少吃。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5811', '25', '0', '1', '0', '0', '', '5811', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232917', '1508232917', '1', '0', '0', '', '/archives/5811.html', '1', '1', '1', '', '健身期间如何吃才能有效增肌？', '1508232812', '/d/file/shipu/2017-10-17/4765c684ed1f3ca45ad14523a01f9037.jpg', '', '如今，越来越多的人选择健身来保持健康的身材。了解健身信息的朋友们都知道，健身不止在于练，更在于吃。也就是说，选择合适的增肌食物，在正确的时间吃它们，健身的效果会更加明显。那么，健身期间到底该如何吃才能有效增肌呢？小编将在接下来为大家解答。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5723', '3', '0', '10', '0', '0', '', '5723', '2', 'adminn.cn', '1', '0', '1', '0', '0', '0', '0', '1507618355', '1507618477', '1', '0', '0', '', '/archives/5723.html', '1', '1', '1', '', ' 冉莹颖健身照展现专业感（图）', '1507617781', '/d/file/pic/2017-10-10/44163d6d76268d47540a13ee0f390f68.jpg', '', '近日，冉莹颖在个人社交平台晒出一组训练健身照片，一身拳击装扮的她显得专业感十足……', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5774', '1', '0', '1', '0', '0', '', '5774', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508221055', '1508221055', '1', '0', '0', '', '/archives/5774.html', '1', '1', '1', '', '这么多人学习健身教练，未来发展空间究竟有多大？', '1508220980', '/d/file/jiaolian/2017-10-17/94353f81a3b0d2e9c91c9e0c12e96975.jpg', '', '健身行业当下的发展势如破竹，健身教练在强大行业形势的影响之下，已经成为了火热的职业。现在很多走进健身行业的人都说，健身行业的压力大，其实是专业压力大。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5731', '3', '0', '0', '0', '0', '', '5731', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1507691293', '1507691293', '1', '0', '0', '', '/archives/5731.html', '1', '1', '1', '', '10月健身让身体让肌肉突显让臀部更翘', '1507690434', '/d/file/pic/2017-10-11/760565abdeaa70976cb0e6aadfa5882a.jpg', '', '每一次呼吸都是上天的恩赐啊，能活着本身就是一大乐事。身体，从来没有一种坚持会被辜负，健身的成本和阅读一样低到令人发指，可能是最廉价的自我提升方法之一。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5810', '25', '0', '7', '0', '0', '', '5810', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232809', '1508232809', '1', '0', '0', '', '/archives/5810.html', '1', '1', '1', '', '增肌营养食谱增肌吃什么最好', '1508232662', '/d/file/shipu/2017-10-17/dee61ff76c61b7b503973d8c4b37bfa9.jpg', '', '健身增肌除了正确的运动，还要有配套的增肌营养食谱才行哦！那么增肌营养食谱有哪些呢？健身增肌吃什么是最好的？当然能吃也有不能吃的，健身增肌哪些是不能碰的呢？一起了解下！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5839', '15', '0', '4', '0', '0', '', '5839', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508383108', '1508383108', '1', '0', '0', '', '/archives/5839.html', '1', '1', '1', '', '上斜飞鸟动作的两种器械训练，帮你练出胸肌中缝', '1508382990', '/d/file/qicai/2017-10-19/3e1d7072ba832966ae0c565b2cee303d.jpg', '', '如果你想要拥有一个完美的胸肌，那么胸肌中缝是必不可少的，只有练出明显的胸肌中缝，才能让你的胸部更加立体有型。想要一个完美的胸型，就必须练出你的胸肌中缝。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5809', '25', '0', '0', '0', '0', '', '5809', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232659', '1508232659', '1', '0', '0', '', '/archives/5809.html', '1', '1', '1', '', '增肌饮食让你短时间内看到肌肉惊人的变化', '1508232510', '/d/file/shipu/2017-10-17/674acc2401d33802cd5f04666606e421.jpg', '', '这些酶在蛋白质的代谢里具有重要作用，微量元素里的铬可以起到减脂增肌的效果，在加强体内脂肪燃烧的同时，能促进肌肉蛋白的生长……所以，增肌爱好者需强化微量元素的补充，如增加食物的多样性、防止偏食，摄入足量的蔬菜、水果等。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5808', '25', '0', '1', '0', '0', '', '5808', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232507', '1508232507', '1', '0', '0', '', '/archives/5808.html', '1', '1', '1', '', '男性健身增肌少不了的蔬菜食谱', '1508232343', '/d/file/shipu/2017-10-17/c011b89e8407d07b74dc9d83aae0bd87.jpg', '', '生活中，正如女人爱美一样，男人也有爱美之心，不过男人的爱美追求不在于装扮上，而是踏踏实实地健身塑形，拥有一身健壮肌肉是男人健身的终极目的。想要增肌除了坚持的进行健身外，饮食上同样不可忽略，蔬菜的摄入量是很重要的，男性健身的运动食谱中，蔬菜的摄入能够帮助男性起到哪些作用呢？此外，男性健身吃什么食物好？', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5807', '25', '0', '0', '0', '0', '', '5807', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232341', '1508232341', '1', '0', '0', '', '/archives/5807.html', '1', '1', '1', '', '瘦子增肌食谱参考，让你的肌肉长出来！', '1508232095', '/d/file/shipu/2017-10-17/29d983015c4d4654c4a1581725d00906.jpg', '', '肌肉是由众多肌纤维组成的，训练是为了撕裂这些肌纤维，然后吸收摄入的蛋白质，进行”超额恢复“，使肌肉增长壮大。因此，训练的再多，蛋白质摄入不足，肌肉也没有足够的营养去增长。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5806', '25', '0', '0', '0', '0', '', '5806', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508232092', '1508232092', '1', '0', '0', '', '/archives/5806.html', '1', '1', '1', '', '健身增肌食谱，会吃才能长肌肉', '1508231950', '/d/file/shipu/2017-10-17/0f32f0a2e9dcb4e4344b73a245ce4762.jpg', '', '主要健美食品：粗粮 煮土豆 玉米 燕麦片,苹果 橙 桃 香蕉 果汁,各种蔬 菜,豆类 牛奶 酸奶 鸡胸肉 瘦牛肉 鱼肉 鸡蛋（去蛋黄)', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5805', '28', '0', '2', '0', '0', '', '5805', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508231712', '1508231712', '1', '0', '0', '', '/archives/5805.html', '1', '1', '1', '', '徒手健身中想要获得进步必须学会休息和恢复', '1508231543', '/d/file/tushou/2017-10-17/4e56b3dd2ed114677e8e13595d1dceb3.jpg', '', '现实中会有很多人每天花大量的时间和精力用在锻炼上，但是却没有看到很明显的进步，这很有可能是身体的休息不够。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5804', '28', '0', '0', '0', '0', '', '5804', '2', 'adminn.cn', '0', '0', '1', '0', '0', '0', '0', '1508231538', '1508231733', '1', '0', '0', '', '/archives/5804.html', '1', '1', '1', '', '连个哑铃都舍不得还增什么肌！', '1508231429', '/d/file/tushou/2017-10-17/dca4cc8207e0e8d11474ee95bbf957d3.jpg', '', '没错说的就是你，徒手健身虽然迷人，但现实一点也不迷人，想增肌就别被所谓的自重体系蒙蔽了双眼！经常有人和我要所谓的徒手健身计划，想要完全通过自重徒手的方式训练，从而达到心目中所想的健身目标，幻想着肌肉饱满有力，走在大街上仿佛行人都停止了。说真的，是时候醒醒了！', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5724', '2', '0', '33', '0', '0', '', '5724', '2', 'adminn.cn', '0', '1', '1', '0', '0', '0', '0', '1507620355', '1507629577', '1', '0', '0', '', '/archives/5724.html', '1', '1', '1', '', '有了这些健身技巧，掌握训练规律，让你的健身路不迷路！', '1507619708', '/d/file/jihua/2017-10-10/4232791bdc0de631367e945988d1f42e.gif', '', '对于很多的健身小白来说，走进健身房在没有人带领的情况下，其实并不知道到底应该做点什么，首先没有合理的计划和安排，去了也只是照葫芦画瓢，看到别的人大汗淋漓撸铁撸的热火朝天，自己也就只能上上跑步机跑会儿步，做下仰卧起坐之类的简单普及运动。', '0');
INSERT INTO `www_92game_net_souid_ecms_news` VALUES ('5722', '1', '0', '10', '0', '0', '', '5722', '2', 'adminn.cn', '1', '0', '1', '0', '0', '0', '0', '1507616292', '1507617423', '1', '0', '0', '', '/archives/5722.html', '1', '1', '1', '', '平时默默救火的英雄 脱衣后竟比健身教练身材都赞', '1507615839', '/d/file/jiaolian/2017-10-10/15822781e9fd65671848876ca2628708.jpg', '', '最近看了一期《天天向上》，内容主要是邀请了来自五湖四海的消防官兵，他们集体亮相，展现了消防官兵的精神，当然敬佩他们精神的时候，令人目瞪口呆的一刻出现了，他们卸下消防兵的服装，露出令人羡慕的六块腹肌，展现他们坚强的体魄，此刻的你才知道你所向往的真正的男神是什么样的！', '0');

